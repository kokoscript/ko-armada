DJGPP = A:\PROGS\DJGPP
CC = $(DJGPP)\BIN\GCC.EXE
OBJS = MAIN.O DATUM.O P98.O
TARGET = KOARMADA.EXE

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) $(OBJS) -o $(TARGET)

MAIN.O: MAIN.C
	$(CC) -c MAIN.C -o MAIN.O
DATUM.O: DATUM.C
	$(CC) -c DATUM.C -o DATUM.O
P98.O: P98.C:
	$(CC) -c P98.C -o P98.O

clean:
	DEL MAIN.O
	DEL DATUM.O
	DEL P98.O
	DEL KOARMADA.EXE
